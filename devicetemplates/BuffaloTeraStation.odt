<?xml version="1.0" encoding="UTF-8"?>
  <devicetemplate id="buffalots" name="Buffalo TeraStation NAS" priority="20" adddevicename="Buffalo TeraStation" deviceicon="vendors_Buffalo.png">
	<!-- Checks -->
    <check id="ping" meta="ping"/>
    <check id="snmp" meta="snmp" requires="ping"/>
    <check id="snmpbuffalots" meta="snmp" requires="snmp">
      <metadata>
        <oid>1.3.6.1.2.1.1.1.0</oid>
		<checksubstring>TeraStation</checksubstring>
      </metadata>
    </check>
    <check id="snmpbuffalots-if" meta="snmp" requires="snmpbuffalots">
      <metadata>
        <oid>
          1.3.6.1.2.1.2.1.0
        </oid>
      </metadata>
    </check>

    <check id="snmpbuffalots-linuxloadavg" meta="snmp" requires="snmpbuffalots">
      <metadata>
        <oid>
          1.3.6.1.4.1.2021.10.1.3.1
        </oid>
      </metadata>
    </check>
	<check id="snmpbuffalots-linuxmeminfo" meta="snmp" requires="snmpbuffalots">
		<metadata>
		  <oid>1.3.6.1.4.1.2021.4.5.0</oid>
		</metadata>
	</check>
	
    <check id="snmpbuffalots-UCD" meta="snmp" requires="snmpbuffalots">
      <metadata>
        <oid>
          1.3.6.1.4.1.2021.4.1.0
        </oid>
		<checksubstring>0</checksubstring>
      </metadata>
    </check>

	
	<!-- Create Standard SNMP sensors -->
    <create id="snmptraffic" kind="snmptraffic" meta="portscan" requires="snmpbuffalots-if" displayname="NIC Traffic">
      <createdata>
        <tags>
          bandwidthsensor snmptrafficsensor
        </tags>
        <priority>
          3
        </priority>
        <comments/>
        <primarychannel>
          -1
        </primarychannel>
        <position>
          80
        </position>
        <inherittriggers>
          <flags>
            <inherited/>
          </flags>
          <cell>
            1
          </cell>
        </inherittriggers>
        <stack>
          2
        </stack>
        <usesingleget/>
		<!-- NIC Traffic: ([ifalias]) [ifdescr] -->
        <nametemplate>
          NIC Traffic: ([ifalias]) [ifdescr]
        </nametemplate>
        <namein>
          Traffic In
        </namein>
        <nameout>
          Traffic Out
        </nameout>
        <namesum>
          Traffic Total
        </namesum>
        <trafficmode>
          <cell row="0">
            errors
          </cell>
          <cell row="1">
            discards
          </cell>
          <cell row="2">
            multicast
          </cell>
          <cell row="3">
            broadcast
          </cell>
        </trafficmode>
        <monitorstate>
          0
        </monitorstate>
      </createdata>
	</create>
	
	<create id="linuxmeminfo" kind="snmplinuxmeminfo" requires="snmpbuffalots-linuxmeminfo"/>
	<create id="linuxloadavg" kind="snmplinuxloadavg" requires="snmpbuffalots-linuxloadavg"/>
	
	<!-- Check and Create CPU Load sensor -->
    <check id="snmpbuffalots-cpu" meta="snmpcpu" requires="snmpbuffalots"/>
    <create id="buffsnmpcpu" kind="snmpcpu" requires="snmpbuffalots-cpu">
      <createdata>
        <tags>
          snmp cpu cpuloadsensor
        </tags>
        <priority>
          3
        </priority>
        <comments/>
        <primarychannel>
          0
        </primarychannel>
        <position>
          100
        </position>
        <inherittriggers>
          <flags>
            <inherited/>
          </flags>
          <cell>
            1
          </cell>
        </inherittriggers>
        <stack>
          0
        </stack>
        <usesingleget/>
      </createdata>
      <triggerdata/>
    </create>

